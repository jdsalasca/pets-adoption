# Supabase Configuration (do NOT commit actual values)
# Copy this file to .env and fill with your own secrets locally.
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_ACCESS_TOKEN=

# Database Configuration (Supabase PostgreSQL)
# Example (Pooler): jdbc:postgresql://aws-1-us-east-2.pooler.supabase.com:6543/postgres
DATABASE_URL=
DATABASE_USERNAME=
DATABASE_PASSWORD=

# JWT Configuration
# Use a long, random secret (32+ chars). Keep it only in .env
JWT_SECRET=
JWT_EXPIRATION=86400000

# Server Configuration
PORT=8080
# dev uses H2 or local PG, prod connects to Supabase (set env vars above)
SPRING_PROFILES_ACTIVE=dev